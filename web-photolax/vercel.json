{
  "version": 2,
  "build": {
    "env": {
      "API_URL": "https://photolax.onrender.com/api-photolax"
    }
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 